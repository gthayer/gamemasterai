generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model StatBlocks {
  ID       Int     @id @default(autoincrement())
  prompt   String? @db.VarChar(300)
  statBlock String? @db.VarChar(8000)
  model String?
  user Int 
  rating String
}

model Users {
  ID    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
